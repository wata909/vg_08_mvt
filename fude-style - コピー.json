{
  "version": 8,
  "name": "Basic",
  "sources": {
    "fude": {"type": "vector", "url": "fude-tile.json"},
    "gsi-std": {
      "type": "raster",
      "tiles": [
        "https://cyberjapandata.gsi.go.jp/xyz/std/{z}/{x}/{y}.png"
      ],
      "minzoom": 0,
      "maxzoom": 18,
      "attribution": "国土地理院"
    },
    "gsi-pole": {
      "type": "raster",
      "tiles": [
        "https://cyberjapandata.gsi.go.jp/xyz/pale/{z}/{x}/{y}.png"
      ],
      "minzoom": 0,
      "maxzoom": 18,
      "attribution": "国土地理院 / Shoreline data is derived from: United States. National Imagery and Mapping Agency. \"Vector Map Level 0 (VMAP0).\" Bethesda, MD: Denver, CO: The Agency; USGS Information Services, 1997."
    },
    "gsi-ort": {
      "type": "raster",
      "tiles": [
        "https://cyberjapandata.gsi.go.jp/xyz/ort/{z}/{x}/{y}.jpg"
      ],
      "minzoom": 14,
      "maxzoom": 18,
      "attribution": "国土地理院"
    },
    "rapid-raster": {
      "type": "raster",
      "tiles": [
        "https://www.finds.jp/ws/tmc/1.0.0/Kanto_Rapid-900913-L/{z}/{x}/{y}.png"
      ],
      "minzoom": 0,
      "maxzoom": 17
    },
    "gsi-vector": {
      "type": "vector",
      "tiles": [
        "https://cyberjapandata.gsi.go.jp/xyz/experimental_bvmap/{z}/{x}/{y}.pbf"
      ],
      "minzoom": 5,
      "maxzoom": 16
    }
  },
  "layers": [
    {
      "id": "gsi-std",
      "type": "raster",
      "source": "gsi-std",
      "minzoom": 0,
      "maxzoom": 18
    },
    {
      "id": "gsi-pole",
      "type": "raster",
      "source": "gsi-pole",
      "minzoom": 0,
      "maxzoom": 18,
      "layout": {
        "visibility": "none"
      }
    },
    {
      "id": "gsi-ort",
      "type": "raster",
      "source": "gsi-ort",
      "minzoom": 14,
      "maxzoom": 18,
      "layout": {
        "visibility": "none"
      }
    },
    {
      "id": "rapid-raster",
      "type": "raster",
      "source": "rapid-raster",
      "minzoom": 0,
      "maxzoom": 17,
      "layout": {
        "visibility": "none"
      }
    },
    {
      "id": "fude",
      "type": "fill",
      "source": "fude",
      "source-layer": "fude",
      "minzoom": 7,
      "maxzoom": 18,
      "paint": {
        "fill-color": [
          "match",
          ["get", "LC"],
          "田",
          "rgba(255, 255, 0, 0.5)",
          "畑",
          "rgba(138, 95, 46, 0.5)",
          "rgba(0, 0, 0, 0)"
        ],
        "fill-outline-color": {
          "stops": [
            [
              7,
              "rgba(0, 0, 0, 0)"
            ],
            [
              10,
              "rgba(0, 0, 0, 0)"
            ],
            [
              11,
              "rgba(0, 0, 0, 0.5)"
            ]
          ]
        }
      }
    },
    {
      "id": "fude-highlight",
      "type": "fill",
      "source": "fude",
      "source-layer": "fude",
      "minzoom": 7,
      "maxzoom": 18,
      "filter": [
        "in",
        "Poly_ID",
        ""
      ],
      "paint": {
        "fill-color": "rgba(255,140,0,0.3)",
        "fill-outline-color": "rgba(255,140,0,1)"
      }
    },
    {
      "id": "gsi-road",
      "type": "line",
      "source": "gsi-vector",
      "source-layer": "road",
      "minzoom": 5,
      "maxzoom": 18,
      "filter": [
        "all",
    [
      "<",
      "ftCode",
      2705
    ],
    [
      ">",
      "rnkWidth",
      0
    ]
      ],
      "paint": {
        "line-width": 5,
        "line-color": [
          "case",
          [
            "==",
            [
              "get",
              "rdCtg"
            ],
            0
          ],
          "rgba(0, 255, 0, 1)",
          [
            "==",
            [
              "get",
              "rdCtg"
            ],
            1
          ],
          "rgba(255, 0, 0, 1)",
          [
            "==",
            [
              "get",
              "rdCtg"
            ],
            2
          ],
          "rgba(0, 0, 255, 1)",
          "rgba(0, 0, 0, 0)"
        ]
      }
    }
  ],
  "id": "basic"
}